          </m:mrow>
          <m:mfrac>
            <m:mrow>
              <m:mi>t</m:mi>
              <m:mi>i</m:mi>
              <m:mi>a</m:mi>
              <m:mi>l</m:mi>
              <m:mi>u</m:mi>
            </m:mrow>
            <m:mrow>
