                <m:mrow>
                  <m:mo>(</m:mo>
                  <m:msup>
                    <m:mi>t</m:mi>
                    <m:mi>n</m:mi>
                  </m:msup>
                  <m:mo>-</m:mo>
                  <m:msup>
                    <m:mi>t</m:mi>
                    <m:mrow>
                      <m:mi>n</m:mi>
