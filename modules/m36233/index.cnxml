            </m:mtd>
            <m:mtd columnalign="left">
              <m:mrow>
                <m:mn>4</m:mn>
                <m:mi>M</m:mi>
                <m:mi>δ</m:mi>
                <m:mo>(</m:mo>
                <m:mi>b</m:mi>
                <m:mo>-</m:mo>
