            <m:mtd columnalign="left">
              <m:mrow>
                <m:mn>2</m:mn>
                <m:mi>M</m:mi>
                <m:mo>(</m:mo>
                <m:msubsup>
                  <m:mo>∫</m:mo>
                  <m:mi>a</m:mi>
                  <m:mi>b</m:mi>
                </m:msubsup>
                <m:mo>|</m:mo>
                <m:mi>l</m:mi>
                <m:mrow>
                  <m:mo>(</m:mo>
                  <m:mi>t</m:mi>
                  <m:mo>)</m:mo>
                </m:mrow>
                <m:mo>-</m:mo>
