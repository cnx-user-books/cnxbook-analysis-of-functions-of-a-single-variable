                <m:mfrac>
                  <m:mn>1</m:mn>
                  <m:mrow>
                    <m:mn>2</m:mn>
                    <m:mi>π</m:mi>
                    <m:mi>i</m:mi>
                  </m:mrow>
                </m:mfrac>
                <m:msub>
                  <m:mo>∫</m:mo>
                  <m:msub>
